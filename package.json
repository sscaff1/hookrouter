{
  "name": "@ui/hookrouter",
  "version": "1.0.1",
  "description": "A hook based router for React",
  "main": "umd/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx --fix",
    "test": "jest",
    "build": "npm run clean && npm run copy && rollup -c",
    "devPackage": "npm run lint && npm run test && npm run build && cd dist && npm pack",
    "clean": "shx rm -rf dist",
    "copy": "shx mkdir dist && shx cp -r src dist/src && shx cp {package.json,README.md,.npmignore} dist && shx cp -r types dist",
    "publishMe": "npm publish ./dist"
  },
  "publishConfig": {
    "registry": "http://ux-nexus.saas-n.com/repository/ux-npm/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.bottomline.tech:7999/ui/hookrouter.git"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "devDependencies": {
    "@glu/babel-preset": "^1.3.0",
    "@glu/eslint-config": "^4.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "babel-eslint": "^10.1.0",
    "jest": "24.6.0",
    "react": "^16.8.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.56.3",
    "shx": "^0.3.3"
  }
}
